<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="CalculatorArithmeticSuite" verbose="1">

    <listeners>
    <listener class-name="listeners.TestListener"></listener>
    </listeners>

    <test name="Arith">
        <classes>
            <class name="CalculatorSubTest">
            </class>
            <class name="CalculatorSumTest">
            </class>
            <class name="CalculatorSumTestDouble"></class>
            <class name="CalculatorDivTest"></class>
            <class name="CalculatorDivTestDouble"></class>
            <class name="CalculatorMultTest"></class>
            <class name="CalculatorMultTestDouble"></class>
